<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
<nav class="log-nav">
<%= link_to " ", "/", class: "fa-solid fa-arrow-left" %>
<h2>LOGIN</h2>
<%= f.submit "Log In" %>
</nav>
<%= f.label :email, "Email", class: "label" %>
  <div class="sign-field">
    <%= f.email_field :email, autofocus: true, placeholder:" Email", autocomplete: "email" %>
  </div>

  <div class="sign-field">
<%= f.label :password, "Password", class: "label" %>
    <%= f.password_field :password, placeholder:" Password", autocomplete: "current-password" %>
  </div>

  <% if devise_mapping.rememberable? %>
    <div class="box-field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end %>

  <div class="actions">

  </div>
<% end %>

<%= render "devise/shared/links" %>
